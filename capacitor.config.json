{
  "appId": "com.seudominio.calculadora.listas",
  "appName": "Calculadora de Listas",
  "webDir": ".",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": true,
      "backgroundColor": "#2c3e50"
    },
    "Permissions": {
      "WRITE_EXTERNAL_STORAGE": {
        "name": "WRITE_EXTERNAL_STORAGE"
      }
    }
  },
  "android": {
    "allowMixedContent": true,
    "webContentsDebuggingEnabled": true
  }
}